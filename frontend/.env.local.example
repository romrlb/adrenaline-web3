# Ticket contract address
NEXT_PUBLIC_TICKET_CONTRACT_ADDRESS=your_contract_address_here

# URLs RPC for different networks
NEXT_PUBLIC_SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/example_api_key

# Network information
NEXT_PUBLIC_EXPLORER_URL=https://etherscan.io

# RainbowKit configuration
NEXT_PUBLIC_RAINBOW_PROJECT_ID=your_project_id_here 

# Network configuration - change this value to switch between environments
NEXT_PUBLIC_NETWORK=hardhat

# Block number to start the contract deployment
NEXT_PUBLIC_FROM_BLOCK=0 

# Supabase configuration for Edge Functions
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# En développement, utiliser des données de simulation au lieu des appels blockchain réels
NEXT_PUBLIC_USE_MOCK=false 